@model IEnumerable<Models.ApplicantRelationBO>

<table class="table" id="ApplicantRelationDetailsGrid">
    <thead>
        <tr class="TableHeader">
            <th style="width:50px;vertical-align:middle;padding-left:20px">
                #
            </th>
            <th style="vertical-align:middle;padding-left:20px">
                Name
            </th>
            <th style="vertical-align:middle;padding-left:20px">
                Relationship
            </th>
            <th style="vertical-align:middle;padding-left:20px">
                Status
            </th>
            <th style="vertical-align:middle;padding-left:20px">
                Action
            </th>
        </tr>
    </thead>
    <tbody style="background-color: white" id="tbody">
        @{
            var count = 1;
        }
        @foreach (var item in Model)
        {
            <tr style='height:50px;'>
                <td style='width:20px;vertical-align:middle;'>
                    <span class='label label-info'>
                        @(count++)
                    </span>
                </td>
                <td style="cursor:pointer">@item.RelativeName</td>
                <td>@item.Relationship</td>
                <td>@item.IsActive</td>
                <td style='width:100px;vertical-align:middle;' class='menu-action'>
                    <a href='#custom-modal' data-animation='corner' data-plugin='custommodal'><i class="mdi mdi-pencil" onclick='EditRelationData(@item.RelationId);'></i> </a>&nbsp;&nbsp;
                    <a href='#custom-modal' data-animation='corner' data-plugin='custommodal'><i class="mdi mdi-delete" onclick='DeleteRelationData(@item.RelationId);'></i> </a>
                </td>
            </tr>
        }
    </tbody>
</table>










