@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="wrapper">
    <div class="container-fluid">
        <!-- Page title box -->
        <div class="page-title-alt-bg"></div>
        <div class="page-title-box">
            <ol class="breadcrumb float-right">
                <li class="breadcrumb-item"><a href="javascript:void(0);">Reports</a></li>
                <li class="breadcrumb-item active">Rejected Jobs</li>
            </ol>
            <h4 class="page-title"><i style="color:olive;" class="mdi mdi-home-variant"></i> Rejected Jobs</h4>
        </div>
    </div>

    <div class="col-lg-12">
        <div class="card-box">
            <div class="form-group row">
                <div style="width:99%;" align="right">
                    <div class="table-responsive">
                        <div class="table table-bordered table-striped" id="RejectedJobsList">

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        LoadGrid();
    });



    function LoadGrid() {
        //ShowLoader();
        $.ajax({
            type: "GET",
            url: '../RejectedJobsList/GetRejectedJobsList',
            data: {
            },
            success: function (data) {
                $("#RejectedJobsList").html(data);
                $('#RejectedJobsGrid').DataTable();
                setTimeout(function () {
                    HideLoader();
                }, 500);
            }
        });
    }
</script>