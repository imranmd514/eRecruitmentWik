@model IEnumerable<Models.SelectedApplicantsListBO>

<table class="table" id="SelectedApplicantsListGrid">
    <thead>
        <tr class="TableHeader">
            <th style="width:50px;vertical-align:middle;padding-left:20px">
                #
            </th>
            <th style="text-align:center">
                Applicant Name
            </th>
            <th style="text-align:center">
                Qualification
            </th>
            <th style="text-align:center">
                Mobile number
            </th>
            <th style="text-align:center">
                Email address
            </th>
            <th style="text-align:center">
                Mother tongue
            </th>
            <th style="text-align:center">
                DOB
            </th>
            <th style="text-align:center">
                Gender
            </th>
            <th style="text-align:center">
                Programme
            </th>
            <th style="text-align:center">
                Name of professional body
            </th>
            <th style="text-align:center">
                Membership Number
            </th>
            <th style="text-align:center">
                Validity of Membership
            </th>
            <th style="text-align:center">
                Subject
            </th>
            <th style="text-align:center">
                Status
            </th>
            @*<th style="vertical-align:middle;padding-left:20px">
                Applicant Name
            </th>
            <th style="vertical-align:middle;padding-left:20px">
                Mobile Number
            </th>
            <th style="vertical-align:middle;padding-left:20px">
                Email Address
            </th>
            <th style="vertical-align:middle;padding-left:20px">
                Address
            </th>
            <th style="text-align:center">
                Location
            </th>
            <th style="text-align:center">
                Status
            </th>
            <th style="text-align:center">
                Donor Program
            </th>
            <th>
                Required staff level
            </th>
            <th>
                Current Staff Level
            </th>
            <th>
                Gaps
            </th>
            <th>
                Comments
            </th>*@
        </tr>
    </thead>
    <tbody style="background-color: white" id="tbodyUser">
        @{
            var count = 1;
        }
        @foreach (var item in Model)
        {
            <tr style='height:50px;'>
                <td style='width:20px;vertical-align:middle;'>
                    <span class='label label-info'>
                        @(count++)
                    </span>
                </td>
                <td style="text-align:center">@item.FullName</td>
                <td style="text-align:center">@item.AcademicQualification</td>
                <td style="text-align:center">@item.PhoneNumber</td>
                <td style="text-align:center">@item.EmailAddress</td>
                <td style="text-align:center">@item.MotherTongue</td>
                <td style="text-align:center">@item.DateOfBirth</td>
                <td style="text-align:center">@item.Gender</td>
                <td style="text-align:center">@item.DonorProgram</td>
                <td style="text-align:center">@item.ProfessionalBodyName</td>
                <td style="text-align:center">@item.MembershipNumber</td>
                <td style="text-align:center">@item.Validity</td>
                <td style="text-align:center">@item.Subject</td>
                <td style="text-align:center">@item.Status</td>



                @*<td style="cursor:pointer">@item.FullName</td>
                <td>@item.PhoneNumber</td>
                <td>@item.EmailAddress</td>
                <td>@item.Address</td>
                <td>@item.JobLocation</td>
                <td>@item.Status</td>
                <td>@item.DonorProgram</td>
                <td>@item.RequiredStaffLevel</td>
                <td>@item.CurrentStaffLevel</td>
                <td>@item.Gaps</td>
                <td>@item.HOD_Comments</td>*@
            </tr>
        }
    </tbody>
</table>







