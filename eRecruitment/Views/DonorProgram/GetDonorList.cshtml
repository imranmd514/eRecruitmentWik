@model IEnumerable<Models.DonorProgramBO>

<table class="table" id="DonorGrid">
    <thead>
        <tr class="TableHeader" style="text-align:center;">

            <th style="width:50px;vertical-align:middle;padding-left:30px;">
                #
            </th>
            <th style="vertical-align:middle;">
                <b> Donor Program</b>
            </th>
            <th style="vertical-align:middle;">
                <b>Is Active</b>
            </th>
            <th style="vertical-align:middle;">
                <b> Action</b>
            </th>
    </thead>

    <tbody id="tbodyOrganization" style="text-align:center;">
        @{
            var count = 1;
        }
        @foreach (var item in Model)
            {
            <tr id='@item.DonorProgramId'>
                <td>@(count++)</td>
                <td style="cursor:pointer" onclick="Edit(@item.DonorProgramId);"> @item.DonorProgram</td>
                <td>@item.IsActive</td>
                <td style="cursor:pointer" onclick="deleteDonor(@item.DonorProgramId,'DonorProgram');"><i class="mdi mdi-delete"></i></td>
            </tr>
        }
    </tbody>
</table>








