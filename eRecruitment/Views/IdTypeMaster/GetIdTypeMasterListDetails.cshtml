@model IEnumerable<Models.IdTypeMasterBO>

<table class="table" id="IdTypeMasterGrid">
    <thead>
        <tr class="TableHeader" style="text-align:center;">

            <th style="width:50px;vertical-align:middle;padding-left:30px;">
                #
            </th>
            <th style="vertical-align:middle;">
                <b>Id Type</b>
            </th>
            <th style="vertical-align:middle;">
                <b>Is Active</b>
            </th>
            <th style="vertical-align:middle;">
                <b>Action</b>
            </th>
    </thead>

    <tbody id="tbodyIdTypeMaster" style="text-align:center;">
        @{
            var count = 1;
        }
        @foreach (var item in Model)
            {
            <tr id='@item.IdTypeMasterId'>
                <td>@(count++)</td>
                <td style="cursor:pointer" onclick="EditIdTypeMaster(@item.IdTypeMasterId);"> @item.IdType</td>
                <td>@item.IsActive</td>
                <td style="cursor:pointer" onclick="DeleteIdTypeMaster(@item.IdTypeMasterId,'IdTypeMaster');"><i class="mdi mdi-delete"></i></td>
            </tr>
        }
    </tbody>
</table>









